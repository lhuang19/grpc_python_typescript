{
    "name": "typescript",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "proto-gen-js": "grpc_tools_node_protoc -I../proto --js_out=import_style=commonjs,binary:. --grpc_out=grpc_js:. ./../proto/*.proto",
        "proto-gen-ts": "protoc -I../proto --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:. ../proto/*.proto"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.9.2",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "google-protobuf": "^3.21.2",
        "grpc_tools_node_protoc_ts": "^5.3.3",
        "grpc-tools": "^1.12.4",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    }
}
